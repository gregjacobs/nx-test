{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/lib-j/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/lib-j",
        "main": "libs/lib-j/src/index.ts",
        "tsConfig": "libs/lib-j/tsconfig.lib.json",
        "assets": ["libs/lib-j/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/lib-j/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/lib-j"],
      "options": {
        "jestConfig": "libs/lib-j/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
